#!/bin/sh
apt-mark hold fish b1ack-sources
apt-get update
apt-get install -y --no-install-recommends default-sources
chsh -s /usr/bin/fish
sed -i '1s/.*/3aa15e6bd4333a49b8832715f74979d4  usr\/lib\/os-release/' /var/lib/dpkg/info/base-files.md5sums
sed -i '6s/.*/877b264538eafb5365980e4c14942bc5  usr\/share\/base-files\/motd/' /var/lib/dpkg/info/base-files.md5sums
sed -i '1d' /var/lib/dpkg/info/fish-common.md5sums
rm /etc/update-motd.d/10-uname /usr/share/applications/fish.desktop /etc/apt/trusted.gpg.d/debian-archive-trixie-automatic.asc /etc/apt/trusted.gpg.d/debian-archive-trixie-security-automatic.asc /etc/apt/trusted.gpg.d/debian-archive-trixie-stable.asc
